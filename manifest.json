{
    "manifest_version": 2,
    "name": "Facebook Event WordPress Copy Paster",
    "version": "1.0",
    "description": "Copie un Event de Facebook pour le coller dans un article WordPress",
    "icons": {
        "16": "icons/16x.png",
        "32": "icons/32x.png",
        "48": "icons/48x.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup/index.html",
        "default_title": "Facebook Event WordPress Copy Paster",
        "default_icon": "icons/32x.png"
    },
    "permissions": [
        "activeTab"
    ]
}
